:CartridgeShape
    a sh:NodeShape ;
    sh:targetClass iffBaseEntities:FilterCartridge ;
    sh:property [
        sh:path iffFilterEntities:hasWasteclass ;
        sh:order 10 ;
        sh:nodeKind sh:BlankNode;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:property [
            sh:path ngsild:hasValue ;
            sh:class iffFilterKnowledge:Wasteclass;
            sh:nodeKind sh:IRI;
            sh:minCount 1 ;
            sh:maxCount 1 ;
        ] ;
    ] ;
.